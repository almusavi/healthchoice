<div class ="homepageheight">
<body class="homepage">
	
<p class="homepageheading">Your #1 online resource for Behavioral Health Care Provider Referrals</p>
<div class="centerform">
<%= form_tag providers_path, :method => 'get' do %>

<p>
	<%= text_field  :insurance, params[:insurance], placeholder: "insurance carrier and plan", class: "input-mysize"%>
	<%= text_field_tag :cityzip, params[:cityzip], placeholder: "zip code or city" %>
	<%= button_tag type: 'submit', :name => nil, class: "button my-button", id: "register-button" do%>
		<i class = "fa fa-search" aria_ hidden="true"></i>
	<% end %>
</p>
<% end %>
</div>
</body>
</div>
